<%= semantic_form_for([:admin, publication], :html => { :class => 'publication'}) do |f| %>
  <%= f.inputs do %>
    <%= f.input :slug, :hint => true %>
    <%= f.input :tags, :hint => true %>
    <%= f.input :audiences, :as => :check_boxes, :collection => Guide::AUDIENCES %>
  <% end %>


<% end %>