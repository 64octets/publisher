<table class="formats" summary="List of &ldquo;Published&rdquo; content">
  <thead>
    <tr>
      <th scope="col">Format</th>
      <th scope="col">Business?</th>
      <th scope="col" class="title">Title</th>
      <th scope="col">Video</td>
      <th scope="col" class="datetime">Last updated</th>
      <th scope="col">Assigned to</th>
      <th scope="col">Created by</th>
      <th scope="col">Section</th>
      <th scope="col">Reviews</th>
      <th scope="col">Published by</th>
      <th scope="col" class="nosort">View</th>
      <th scope="col" class="nosort">Edit</th>
      <th scope="col" class="nosort">New edition</th>
      <th scope="col" class="nosort">Preview</th>
    </tr>
  </thead>
  <tbody>
    <%= render :collection => @presenter.published, :partial => 'published_edition', :as => 'publication', :locals => {:tab => :published}  %>
  </tbody>
</table>