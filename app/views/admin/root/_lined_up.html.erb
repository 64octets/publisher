  <table class="table table-striped table-bordered table-condensed" summary="List of &ldquo;Lined Up&rdquo; content">
  <thead>
    <tr>
      <th scope="col">Format</th>
      <th scope="col">Biz?</th>
      <th scope="col" class="span4">Title</th>
      <th scope="col">Updated</th>
      <th scope="col">Assigned to</th>
      <th scope="col">Section</th>
      <th scope="col">Start</th>
      <th scope="col">Preview</th>
    </tr>
  </thead>
  <tbody>
    <%= render :collection => @presenter.lined_up, :partial => 'publication', :locals => {:tab => :lined_up} %>
  </tbody>
</table>

<%= content_for :extra_javascript do %>
  <script type="text/javascript">
    $(function () {
      $("table").tablesorter({ 
        headers: { 
          // increment from 0 for columns you wish to not sort
          0: { sorter: false }, 
          6: { sorter: false }, 
          7: { sorter: false } 
        } 
      });
    });
  </script>
<% end %>