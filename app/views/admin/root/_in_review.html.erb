<h2>In review content</h2>

<table class="table table-striped table-condensed" summary="List of &ldquo;In Review&rdquo; content">
  <thead>
    <tr>
      <th scope="col">Format</th>
      <th scope="col">Biz?</th>
      <th scope="col" class="title">Title</th>
      <th scope="col">Video</th>
      <th scope="col" class="datetime">Last updated</th>
      <th scope="col">Assigned to</th>
      <th scope="col">Created by</th>
      <th scope="col">Section</th>
      <th scope="col">Reviews</th>
      <th scope="col" class="nosort">Edit</th>
      <th scope="col" class="nosort">Preview</th>
    </tr>
  </thead>
  <tbody>
    <%= render :collection => @presenter.in_review, :partial => 'publication', :locals => {:tab => :in_review} %>
  </tbody>
</table>