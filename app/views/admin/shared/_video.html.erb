<%= f.input :video_url, :label => "Video URL", :hint => 'Put the YouTube URL in here if it exists', :input_html => { :class => 'span7' , :disabled => @resource.published? } %>
<%= f.input :video_summary, :label => "Video Summary", :as => :text, :input_html => { :class => 'span7' , :rows => 2, :disabled => @resource.published? } %>